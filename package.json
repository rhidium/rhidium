{
  "name": "rhidium",
  "version": "2.2.0",
  "description": "Quickly and easily create powerful Discord bots using a powerful, opinionated, all-in-one framework.",
  "main": "dist/src/client/index.js",
  "engines": {
    "node": ">=16.6.0"
  },
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "build": "tsc",
    "prebuild": "rimraf dist",
    "clean": "gts clean; rimraf docs",
    "dev": "npm run start:dev",
    "docs": "npx typedoc",
    "docs:watch": "npx typedoc --watch",
    "fix": "gts fix",
    "lint": "gts lint",
    "pretest": "npm run build",
    "posttest": "npm run lint",
    "start": "node --enable-source-maps dist/src/client/index.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --notify --transpile-only src/client/index.ts",
    "test": "mocha dist/test/**/*.js",
    "test:dev": "ts-mocha test/**/*.spec.ts -w --watch-files '**/*.ts'",
    "typecheck": "tsc --noEmit",
    "commit": "cz",
    "setup:linux": "pnpm install && cp .env.example .env && cp config/config.example.json config/config.json",
    "setup:windows": "pnpm install && copy .env.example .env && copy config/config.example.json config/config.json",
    "generate-schema": "npx ts-json-schema-generator --path 'src/core/config/*.ts' --type 'UserConfigOptions' --tsconfig tsconfig.json",
    "update-schema": "node scripts/create-config-schema.mjs",
    "preconfig-editor": "npm run update-schema",
    "config-editor": "node scripts/config-editor.mjs",
    "setup:config": "pnpm add -D @rhidium/json-editor",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "keywords": [
    "discord",
    "template",
    "bot-template",
    "node",
    "discordapp",
    "typescript",
    "rhidium",
    "framework",
    "discord.js"
  ],
  "author": {
    "name": "Richard Hillebrand",
    "email": "me@mirasaki.dev",
    "url": "https://mirasaki.dev"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.7.5",
    "gts": "^6.0.2",
    "prisma": "^6.4.1",
    "rimraf": "^6.0.1",
    "ts-json-schema-generator": "^2.3.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.27.9",
    "typescript": "^5.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rhidium/rhidium.git"
  },
  "bugs": {
    "url": "https://github.com/rhidium/rhidium/issues"
  },
  "homepage": "https://github.com/rhidium/rhidium#readme",
  "dependencies": {
    "@prisma/client": "6.4.1",
    "colors": "^1.4.0",
    "common-tags": "^1.8.2",
    "cron": "^4.1.0",
    "deep-object-diff": "^1.1.9",
    "discord-hybrid-sharding": "^2.2.5",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "i18next": "^24.2.2",
    "module-alias": "^2.2.3",
    "tslib": "^2.8.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "pnpm": {
    "overrides": {
      "undici": ">=6.21.1"
    }
  }
}
