{
  "name": "rhidium",
  "version": "0.0.1",
  "description": "Quickly and easily create powerful Discord bots using a powerful, opinionated, all-in-one framework.",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16.6.0"
  },
  "scripts": {
    "build": "tsup",
    "clean": "gts clean; rimraf docs",
    "dev": "tsup --watch --ignore-watch .vscode --onSuccess \"node dist/index.js\"",
    "docs": "npx typedoc",
    "docs:watch": "npx typedoc --watch",
    "fix": "gts fix",
    "lint": "gts lint",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "posttest": "npm run lint",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "discord",
    "bot",
    "framework",
    "discord.js",
    "discord-bot-template",
    "discord-bot-framework",
    "discord-bot"
  ],
  "author": {
    "name": "Richard Hillebrand",
    "email": "me@mirasaki.dev",
    "url": "https://mirasaki.dev"
  },
  "license": "ISC",
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
    "@types/node": "^22.7.5",
    "gts": "^6.0.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typedoc": "^0.27.9",
    "typescript": "^5.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Mirasaki/discord.js-bot-template.git"
  },
  "bugs": {
    "url": "https://github.com/Mirasaki/discord.js-bot-template/issues"
  },
  "homepage": "https://github.com/Mirasaki/discord.js-bot-template#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "common-tags": "^1.8.2",
    "cron": "^4.1.0",
    "deep-object-diff": "^1.1.9",
    "discord-hybrid-sharding": "^2.2.5",
    "discord.js": "^14.18.0",
    "i18next": "^24.2.2",
    "tslib": "^2.8.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
